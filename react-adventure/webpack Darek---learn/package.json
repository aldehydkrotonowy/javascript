{
  "name": "yet-another-webpack-es6-starterkit",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --watch --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --open --config webpack-dev.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/yet-another-webpack-es6-starterkit.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/yet-another-webpack-es6-starterkit/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    },
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/wbkd/yet-another-webpack-es6-starterkit#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.17.0",
    "image-webpack-loader": "^4.0.0",
    "node-sass": "^4.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "bootstrap": "^4.0.0-alpha.6",
    "html-loader": "^0.5.5",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "owl.carousel": "^2.2.0",
    "perfect-scrollbar": "^1.3.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1"
  }
}
